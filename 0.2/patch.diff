+        p = Person("Mark", "Min Gata 1")
+        self.assertEqual( p.getAddress(), "Min Gata 1" )
+
+    def test_person_04_set_address( self ):
+        p = Person("Mark", "Min Gata 1")
+        p.setAddress( "Annan Gata 2" )
+        self.assertEqual( p.getAddress(), "Annan Gata 2" )
